extends layout

block content
  .container
    .row
      .col-sm-6.col-md-offset-1
        table(class='table table-striped table-condensed')
          thead
            tr
              th= title
              th Login
          tbody
            each steward in steward_list
              tr
                td: a(href=steward.url) #{steward.name}
                td #{steward.login}
