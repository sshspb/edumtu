extends layout

block content
  table(class='table table-striped table-condensed')
    thead
      tr
        th Фамилия И.О.
        th login
        th role
    tbody
      each steward in steward_list
        tr
          td: a(href= '/admin/steward/' + steward._id) #{steward.name}
          td #{steward.login}
          td #{steward.role}
